{
  "name": "ts-boilerplate",
  "version": "0.0.0",
  "description": "Typescript boilerplate with eslint and jest.",
  "private": true,
  "scripts": {
    "start": "node ./dist/app.js",
    "build": "tsc -p tsconfig.json && tsc-alias",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --inspect -- ./src/app.ts",
    "lint": "eslint --fix --ext .js,.ts,.json .",
    "test": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "eslint": "^8.25.0",
    "eslint-plugin-jest": "^27.1.1",
    "jest": "^29.1.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
